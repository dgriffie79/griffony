{
	"compilerOptions": {
		"checkJs": true,
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": true,
		"plugins": [],
		"alwaysStrict": true,
		"esModuleInterop": false,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ESNext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"module": "ESNext",
		"noUnusedLocals": true,
		"allowArbitraryExtensions": true,
	},
	"include": [
		"src/**/*.js",
	],
}