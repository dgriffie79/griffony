{
	"compilerOptions": {
		"checkJs": true,
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"noImplicitThis": false,
		"alwaysStrict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ES2022",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"module": "ES2022"
	},
	"include": [
		"src/**/*.js"
	],
}