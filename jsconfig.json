{
	"compilerOptions": {
		"checkJs": true,
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"noImplicitThis": true,
		"noPropertyAccessFromIndexSignature": true,
		"plugins": [],
		"alwaysStrict": true,
		"esModuleInterop": false,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"target": "ES2022",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"module": "ES2022",
		"noUnusedLocals": true
	},
	"include": [
		"src/**/*.js"
	],
}